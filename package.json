    "morgan": "~1.9.1",
    "body-parser": "^1.19.0",
    "express-fileupload": "^1.1.6-alpha.6",
    "fabric-ca-client": "~1.4.0",
    "fabric-network": "~1.4.0",
    "ipfs-http-client": "^39.0.2"
  },
  "engineStrict": true,
  "author": "Hyperledger",
  "license": "Apache-2.0",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.3.0"
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "test/**"
